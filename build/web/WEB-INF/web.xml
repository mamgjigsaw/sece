<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>saveIndicador</servlet-name>
        <servlet-class>servlet.saveIndicador</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveItem</servlet-name>
        <servlet-class>servlet.saveItem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveVariable</servlet-name>
        <servlet-class>servlet.saveVariable</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveEscala</servlet-name>
        <servlet-class>servlet.saveEscala</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveUsuario</servlet-name>
        <servlet-class>servlet.saveUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveAcceso</servlet-name>
        <servlet-class>servlet.saveAcceso</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>entrar</servlet-name>
        <servlet-class>servlet.entrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>salir</servlet-name>
        <servlet-class>servlet.salir</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>modifUser</servlet-name>
        <servlet-class>servlet.modifUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveRespItem</servlet-name>
        <servlet-class>servlet.saveRespItem</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delegadoxContrato</servlet-name>
        <servlet-class>servlet.delegadoxContrato</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GraficoResultado</servlet-name>
        <servlet-class>servlet.GraficoResultado</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>saveIndicador</servlet-name>
        <url-pattern>/saveIndicador</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveItem</servlet-name>
        <url-pattern>/saveItem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveVariable</servlet-name>
        <url-pattern>/saveVariable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveEscala</servlet-name>
        <url-pattern>/saveEscala</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveUsuario</servlet-name>
        <url-pattern>/saveUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveAcceso</servlet-name>
        <url-pattern>/saveAcceso</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>entrar</servlet-name>
        <url-pattern>/entrar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>salir</servlet-name>
        <url-pattern>/salir</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>modifUser</servlet-name>
        <url-pattern>/modifUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveRespItem</servlet-name>
        <url-pattern>/saveRespItem</url-pattern>
    </servlet-mapping>
    <servlet>
        <display-name>DWR Servlet</display-name>
        <servlet-name>dwr-invoker</servlet-name>
        <servlet-class>org.directwebremoting.servlet.DwrServlet</servlet-class>
        <init-param>
            <param-name>debug</param-name>
            <param-value>true</param-value>
        </init-param>
        <init-param>
            <param-name>activeReverseAjaxEnabled</param-name>
            <param-value>true</param-value>
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>dwr-invoker</servlet-name>
        <url-pattern>/dwr/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delegadoxContrato</servlet-name>
        <url-pattern>/delegadoxContrato</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GraficoResultado</servlet-name>
        <url-pattern>/GraficoResultado</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
        <cookie-config>
            <http-only>false</http-only>
        </cookie-config>
    </session-config>
</web-app>
