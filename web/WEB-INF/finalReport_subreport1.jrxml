	<queryString language="SQL">
		<![CDATA[SELECT
     ( SELECT COUNT(*) FROM `resp_item` resp_item INNER JOIN `item` item ON resp_item.`id_item` = item.`id_item` WHERE resp_item.id_contrato = $P{idContrato} and resp_item.res_boolean = 1 and item.id_variable = variable.id_variable )  Resp_positivas,
