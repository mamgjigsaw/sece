     ( SELECT escala.`descripcion` FROM `escala` escala WHERE escala.valoracion = 0 and escala.id_variable = variable.id_variable ) esc0,
     variable.`nombre` AS Variable
FROM
     `variable` variable where variable.`id_indicador` = $P{id_indicador}]]>
